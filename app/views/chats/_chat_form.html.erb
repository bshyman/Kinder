<div class="chat-form">
  <%= form_for chat, remote: true do |f| %>
    <div class="input-group">
      <span class="input-group-addon">identifier:</span>
      <%= f.text_field :identifier, class: "form-control"%>
    </div>
    <%= f.submit class: "btn btn-primary", id: "create-chat" %>
  <% end %>
</div>
